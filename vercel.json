{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true,
      "preview": false,
      "feature/*": false,
      "feat/*": false,
      "fix/*": false,
      "hotfix/*": false,
      "release/*": false
    }
  },
  "ignoreCommand": "git diff HEAD^ HEAD --quiet ./",
  "installCommand": "pnpm install",
  "buildCommand": "pnpm build",
  "framework": "remix",
  "outputDirectory": "build/client",
  "routes": [
    {
      "src": "/build/.*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "deploymentMetadata": {
    "version": 2,
    "deploymentId": true,
    "buildCommand": true,
    "buildTimestamp": true,
    "debug": true
  },
  "logDrains": {
    "enabled": true
  }
}
